var trackPageInfo = '{"info":{"pageName":"\u623f\u6e90\u5217\u8868\u9875","businessline_pvar":"\u4e8c\u624b\u623f"},"type":"ershoufang","scclick":{"cookie_id_var":"EC0EDD86-FD34-C35A-6726-494157203AD9","platform_var":"pc"}}';
